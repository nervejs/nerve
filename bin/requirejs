const requirejs = require('requirejs');

const config = {
    baseUrl: 'tmp/js/es5',
    name: 'nerve',
    optimize: 'none',
    out: 'dist/nerve.js',
    wrap: {
        startFile: './node_modules/requirejs/require.js'
    },
    paths: {
        axios: '../../../node_modules/axios/dist/axios'
    }
};

requirejs.optimize(config, function (results) {
    console.log(results);
});